{
    "rewrites": [
        {
            "source": "/((?!api/|assets/|sw.js|manifest.json|OneSignalSDKWorker.js|icons/).*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(sw.js|OneSignalSDKWorker.js|workbox-.*.js)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}